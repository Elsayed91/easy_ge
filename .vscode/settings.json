    "hide-files.files": [
        "__pycache__",
        ".pytest_cache",
        "LICENSE",
        "poetry.lock",
        "readme.md",
        ".pre-commit-config.yaml",
        "gcp_key_spark.json",
        ".vscode"
    ]
}