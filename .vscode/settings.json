{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.trunk/*out": true,
        "__pycache__": true,
        ".pytest_cache": true,
        "LICENSE": true,
        "poetry.lock": true,
        "readme.md": true,
        ".pre-commit-config.yaml": true,
        "gcp_key_spark.json": true,
        ".vscode": true
